    <TIER ANNOTATOR="emkartova" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@e1931f"
        PARTICIPANT="@e1931f" TIER_ID="transl@e1931f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a7" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>then there were a lot //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a8" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>then there were a lot //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a9" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>we couldn`t share the haymaking for mowing / yes //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a10" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>and now everything is overgrown //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a11" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>#inaud# / well //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a12" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>yesterday Medvedev / #inaud# said / said on the radio / that / let`s revive rural areas / kolkhoses / yes //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a15" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>because / abroad / they won`t take / from there from there they said they won`t / we need to start our own because we stopped keeping cows //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a17" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>in Nekrashovo Zakruchie also / Zalejie / there were villages / and now... in Nekrashovo there maybe still some / but in Zakruchie i don`t know / maybe all are dead //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a20" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>i moved from Sur... from Zakruchie / to Bel`kovo / got married / and we lived a century //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a21" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>no / our father drowned / in the year thirty-eight in the month of april //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>before the war yeah / they had made a ton of us //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>i had two brothers / four sisters / that`s how many of us there were //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>we were all left without a father befor the war //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>yes / lady Manya Nikonova had / seven of them //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>well her... son / had died befor the war / Vit`ka / yeah... there are books / yeah... the book / how do i say this / i told Tanya / i don`t know whether  she gave Leshka the book or not / Rita gave it to me //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE>i gave it to Kol`ka to read and he / him... got ill / and i don`t know where the book is / i asked her recently / she says unharmed with me //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>i was born in the thirty-first year //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE>august the fifth //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE>grandmother is from Bel`kovo / if father is from Bel`kovo then grandmother is also from Bel`kovo //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE>so their relatives were / g... father / over there #inaud# schoolmistress // </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE>did you visit school? //</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
